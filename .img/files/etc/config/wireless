
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/d0000000.apb/d0070000.mmc/mmc_host/mmc2/mmc2:0001/mmc2:0001:1'
	option disabled '0'
	option country '00'
	option legacy_rates '1'
	option mu_beamformer '0'
	option hwmode '11a'
	option channel '36'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'WiFi'
	option encryption 'psk2'
	option key 'password'
	option network 'lan'

